{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev:linux": "concurrently \"redis-server\" \"bun sass\" \"nuxt dev\"",
    "dev:windows": "concurrently \"npm run sass\" \"nuxt dev\"",
    "dev": "node ./run-dev.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "sass": "sass assets/scss/main.scss  assets/css/main.css --watch",
    "prepare": "node ./setup.js",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.11.2",
    "concurrently": "^8.2.2",
    "nuxt": "^3.10.0",
    "sass": "^1.70.0",
    "vitest": "^1.2.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  }
}
