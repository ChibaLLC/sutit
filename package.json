{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev:linux": "concurrently \"redis-server\" \"bun sass\" \"nuxt dev --host\"",
    "dev:windows": "concurrently \"npm run sass\" \"nuxt dev --host\"",
    "dev": "run || ./run.sh",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "sass": "sass assets/scss/main.scss  assets/css/main.css --watch",
    "prepare": "setup || chmod +x ./setup.sh && ./setup.sh && chmod +x ./run.sh",
    "test": "vitest",
    "introspect": "drizzle-kit introspect:pg",
    "migrate": "drizzle-kit push:pg"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.11.2",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.14",
    "nuxt": "^3.10.0",
    "sass": "^1.70.0",
    "vitest": "^1.2.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@chiballc/nuxt-form-builder": "0.0.26",
    "daraja.js": "github:kgarchie/daraja.js#c8035ef849eb7d0de358f41fc14acc960a1c9a1c",
    "drizzle-orm": "^0.29.3",
    "object.groupby": "^1.0.2",
    "postgres": "^3.4.4",
    "std-env": "^3.7.0",
    "ulid": "^2.3.0",
    "uuid": "^9.0.1"
  }
}
