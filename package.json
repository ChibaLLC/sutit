{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev:linux": "concurrently \"redis-server\" \"bun sass\" \"nuxt dev\"",
    "dev:windows": "concurrently \"npm run sass\" \"nuxt dev\"",
    "dev": "node ./run-dev.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "sass": "sass assets/scss/main.scss  assets/css/main.css --watch",
    "prepare": "node ./setup.js",
    "test": "vitest",
    "introspect": "drizzle-kit introspect:mysql"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.11.2",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.14",
    "nuxt": "^3.10.0",
    "sass": "^1.70.0",
    "vitest": "^1.2.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.3",
    "mysql2": "^3.9.1",
    "qrcode": "^1.5.3",
    "ulid": "^2.3.0",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "^1.23.0"
  }
}
