{
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "ui:\\s*{([^)]*)\\s*}",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "/\\*ui\\*/\\s*{([^;]*)}",
            ":\\s*[\"'`]([^\"'`]*).*?[\"'`]"
        ]
    ],
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "ui"
    ],
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 3306,
            "driver": "MariaDB",
            "name": "Sutit",
            "database": "sutit",
            "username": "root",
            "password": ""
        }
    ],
    "dotenv.enableAutocloaking": false
}